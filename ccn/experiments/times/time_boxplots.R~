######Total Transmission Time##################################################
errf <- function(array){

    object <- 1.64*sd(array)/sqrt(length(array))

    return(object)
}

files = list.files("./time_files/")

file_names = c()
for (file in files){
  print(file)
  name = paste("./time_files/",file,sep="")
  file_names = c(file_names,name)
}

d1 = read.csv(file_names[1],head=F)$V1
d2 = read.csv(file_names[2],head=F)$V1
d3 = read.csv(file_names[3],head=F)$V1
d4 = read.csv(file_names[4],head=F)$V1
d5 = read.csv(file_names[5],head=F)$V1
d6 = read.csv(file_names[6],head=F)$V1

pdf("time_boxplots.R")
boxplot(d1,d2,d3,d4,d5,d6,names=c("Alg.1 (PKE)","Alg.2 (PKE)","Alg.1 (Symm)","Alg.2 (Symm)","Alg.3","Alg.4"), ylab= "Computation Time (us)", xlab= "CCVPN Algorithms")
